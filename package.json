{
  "name": "parcelLab-js-plugin",
  "version": "1.0.0",
  "description": "JavaScript plugin used to retrieve and display trackings from parcelLab on any given webpage.",
  "main": "src/js/index.js",
  "scripts": {
    "start": "npm-run-all -p dev:*",
    "test": "make test",
    "build": "make bundle",
    "deploy": "make deploy",
    "dev:serve-test": "serve test/ -p 4000",
    "dev:serve-bundle": "serve build/ -p 4001",
    "dev:watch-build": "webpack -d --progress --colors --watch"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/parcelLab/parcelLab-js-plugin.git"
  },
  "keywords": [
    "parcellab",
    "trackings"
  ],
  "author": "Julian Krenge, Oscar Reyes, Andrej Fritz",
  "license": "MIT",
  "homepage": "",
  "devDependencies": {
    "babel-core": "^6.11.4",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-es2015-minimal": "^2.1.0",
    "babel-preset-stage-0": "^6.5.0",
    "css-loader": "^0.25.0",
    "expose-loader": "^0.7.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "handlebars-loader": "^1.3.0",
    "hogan.js": "^3.0.2",
    "json-loader": "^0.5.4",
    "mustache-loader": "^0.3.1",
    "node-sass": "^3.4.2",
    "npm-run-all": "^3.0.0",
    "raw-loader": "^0.5.1",
    "sass-loader": "^4.0.2",
    "webpack": "^1.13.0"
  },
  "dependencies": {
    "cash-dom": "^1.3.4",
    "country-language": "^0.1.7",
    "countryjs": "^1.5.2",
    "raven-js": "^3.5.1"
  },
  "jshintConfig": {
    "esnext": true,
    "eqeqeq": true,
    "forin": true,
    "undef": true,
    "unused": true,
    "latedef": true,
    "nocomma": true,
    "predef": [
      "console",
      "require",
      "document",
      "XMLHttpRequest",
      "process",
      "window",
      "localStorage",
      "module",
      "exports",
      "describe",
      "navigator",
      "it",
      "setTimeout"
    ]
  }
}
